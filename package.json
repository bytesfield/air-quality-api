{
  "name": "air-quality",
  "version": "1.0.0",
  "description": "Air Quality Api integration",
  "private": false,
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "testMatch": [
      "**/src/**/*.test.ts"
    ],
    "setupFiles": [
      "dotenv/config"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "transform": {
      "^.+\\.[jt]sx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    }
  },
  "scripts": {
    "start": "npm run build && node dist/src/server",
    "deploy": "npm install && npm run build && npm run migrate",
    "build": "tsc",
    "start:dev": "ts-node-dev --files --respawn ./src/server.ts",
    "test": "jest --forceExit",
    "lint": "eslint --ext .ts,.js src/**",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier \"**/*.+(ts|js|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --check",
    "check-types": "tsc --noEmit",
    "validate": "npm run check-format && npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytesfield/air-quality-api.git"
  },
  "author": "Abraham Udele",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bytesfield/air-quality-api/issues"
  },
  "homepage": "https://github.com/bytesfield/air-quality-api#readme",
  "dependencies": {
    "axios": "^0.21.4",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.24",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.14.9",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "sequelize-cli": "^6.2.0",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
